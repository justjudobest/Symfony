{% extends 'base.html.twig' %}

{% block title %}AttributeProduct index{% endblock %}

{% block body %}
    <h1>AttributeProduct index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Product_id</th>
                <th>Attribute_id</th>
                <th>Value</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for attribute_product in attribute_products %}
            <tr>
                <td>{{ attribute_product.id }}</td>
                <td>{{ attribute_product.productId }}</td>
                <td>{{ attribute_product.attributeId }}</td>
                <td>{{ attribute_product.value }}</td>
                <td>
                    <a href="{{ path('attribute_product_show', {'id': attribute_product.id}) }}">show</a>
                    <a href="{{ path('attribute_product_edit', {'id': attribute_product.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('attribute_product_new') }}">Create new</a>
{% endblock %}
